{
  "name": "foundryvtt-docker",
  "version": "1.0.0",
  "description": "Tadaaa",
  "repository": "git@github.com:mbround18/foundryvtt-docker.git",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf -rf ./dist/server",
    "build:server": "webpack -c ./config/webpack.config.server.js --mode production",
    "build:client": "nuxt generate",
    "build": "yarn build:server && yarn build:client",
    "start": "node ./dist/server/server.js",
    "dev": "nuxt",
    "release": "auto shipit"
  },
  "nodemonConfig": {
    "exec": "yarn start",
    "ext": "js,json,html",
    "ignore": [
      "./dist"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.16.3",
    "axios": "0.24.0",
    "express": "4.17.1",
    "express-fileupload": "1.2.1",
    "express-rate-limit": "5.5.1",
    "follow-redirects": "1.14.5",
    "lodash": "4.17.21",
    "nuxt": "2.15.7",
    "shelljs": "0.8.4",
    "vue": "2.6.12",
    "vuetify": "2.6.0",
    "vuex": "3.6.2"
  },
  "optionalDependencies": {
    "fibers": "^5.0.0"
  },
  "devDependencies": {
    "@auto-it/core": "10.32.3",
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.4",
    "@babel/preset-env": "7.16.4",
    "@nuxtjs/vuetify": "1.12.1",
    "@yarnpkg/pnpify": "3.1.0",
    "auto": "10.32.3",
    "babel-loader": "8.2.3",
    "consola": "2.15.3",
    "debug": "4.3.2",
    "html-loader": "3.0.1",
    "html-webpack-plugin": "5.5.0",
    "nodemon": "2.0.15",
    "rimraf": "3.0.2",
    "sass": "1.43.5",
    "sass-loader": "10.2.0",
    "semver": "7.3.5",
    "supports-color": "9.2.1",
    "vue-template-compiler": "2.6.12",
    "webpack": "5.64.4",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.6.0",
    "webpack-node-externals": "3.0.0",
    "workbox-webpack-plugin": "6.4.1"
  }
}
