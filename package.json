{
  "name": "foundryvtt-docker",
  "version": "1.0.0",
  "description": "Tadaaa",
  "repository": "git@github.com:mbround18/foundryvtt-docker.git",
  "license": "MIT",
  "targets": {
    "frontend": {
      "context": "browser",
      "source": "client/index.html"
    },
    "backend": {
      "context": "node",
      "source": "server/index.mjs",
      "includeNodeModules": true
    }
  },
  "alias": {
    "process": false
  },
  "scripts": {
    "clean": "rimraf -rf ./dist ./server/dist",
    "build": "parcel build --dist-dir ./dist",
    "start:frontend": "parcel watch --dist-dir ./server/dist --target frontend",
    "start:backend": "node ./server/index.mjs",
    "start": "concurrently --colors green,blue \"npm:start:*\"",
    "release": "auto shipit"
  },
  "nodemonConfig": {
    "exec": "yarn start",
    "ext": "js,json,html",
    "ignore": [
      "./dist"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.17.9",
    "assert": "2.0.0",
    "axios": "0.26.1",
    "follow-redirects": "1.14.9",
    "h3": "0.7.4",
    "lodash": "4.17.21",
    "serve-static": "1.15.0",
    "url": "0.11.0"
  },
  "devDependencies": {
    "@auto-it/core": "10.36.5",
    "@parcel/transformer-sass": "2.4.1",
    "@parcel/transformer-vue": "2.4.1",
    "@types/lodash": "4.14.181",
    "@yarnpkg/pnpify": "3.1.1",
    "auto": "10.36.5",
    "autoprefixer": "10.4.4",
    "concurrently": "7.1.0",
    "crypto-browserify": "3.12.0",
    "nodemon": "2.0.15",
    "os-browserify": "0.3.0",
    "parcel": "2.4.1",
    "path-browserify": "1.0.1",
    "postcss": "8.4.12",
    "querystring-es3": "0.2.1",
    "rimraf": "3.0.2",
    "sass": "1.50.1",
    "sass-loader": "12.6.0",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "tailwindcss": "3.0.24"
  }
}
