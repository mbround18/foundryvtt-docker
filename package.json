{
  "name": "foundryvtt-docker",
  "version": "1.0.0",
  "description": "Tadaaa",
  "repository": "git@github.com:mbround18/foundryvtt-docker.git",
  "license": "MIT",
  "targets": {
    "frontend": {
      "context": "browser",
      "source": "client/index.html"
    },
    "backend": {
      "context": "node",
      "source": "server/index.ts",
      "includeNodeModules": true
    }
  },
  "alias": {
    "process": false
  },
  "scripts": {
    "clean": "rimraf -rf ./dist ./server/dist",
    "build": "parcel build --dist-dir ./dist",
    "compile": "yarn dlx pkg --target node18-linux-x64 dist/backend/index.js --out-path dist/bin",
    "start:frontend": "parcel watch --dist-dir ./server/dist --target frontend",
    "start:backend": "node server/index.ts",
    "start": "concurrently --colors green,blue \"npm:start:*\"",
    "release": "auto shipit"
  },
  "nodemonConfig": {
    "exec": "yarn start",
    "ext": "js,json,html",
    "ignore": [
      "./dist"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.23.7",
    "assert": "2.1.0",
    "axios": "1.6.4",
    "express": "^4.18.2",
    "follow-redirects": "1.15.4",
    "lodash": "4.17.21",
    "url": "0.11.3"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.10.3",
    "@parcel/transformer-vue": "2.10.3",
    "@types/lodash": "4.14.202",
    "autoprefixer": "10.4.16",
    "buffer": "6.0.3",
    "concurrently": "8.2.2",
    "crypto-browserify": "3.12.0",
    "nodemon": "3.0.2",
    "os-browserify": "0.3.0",
    "parcel": "2.10.3",
    "path-browserify": "1.0.1",
    "postcss": "8.4.33",
    "querystring-es3": "0.2.1",
    "rimraf": "5.0.5",
    "sass": "1.69.7",
    "sass-loader": "13.3.3",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "tailwindcss": "3.4.0"
  },
  "packageManager": "yarn@4.0.2"
}
