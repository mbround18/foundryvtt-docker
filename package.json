{
  "name": "foundryvtt-docker",
  "version": "1.0.0",
  "description": "Tadaaa",
  "repository": "git@github.com:mbround18/foundryvtt-docker.git",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf -rf ./dist/server",
    "build:server": "webpack -c ./config/webpack.config.server.js --mode production",
    "build:client": "nuxi build",
    "build": "yarn build:server && yarn build:client",
    "start": "node ./dist/server/server.js",
    "dev": "nuxi",
    "release": "auto shipit"
  },
  "nodemonConfig": {
    "exec": "yarn start",
    "ext": "js,json,html",
    "ignore": [
      "./dist"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.17.2",
    "axios": "^0.25.0",
    "follow-redirects": "^1.14.8",
    "lodash": "^4.17.21",
    "shelljs": "^0.8.5",
    "vue": "^3.2.31",
    "vuetify": "^3.0.0-alpha.12",
    "vuex": "^4.0.2"
  },
  "optionalDependencies": {
    "fibers": "^5.0.1"
  },
  "devDependencies": {
    "@auto-it/core": "10.32.6",
    "@babel/core": "7.17.2",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@vue/server-renderer": "^3.2.30",
    "@yarnpkg/pnpify": "3.1.0",
    "auto": "^10.32.6",
    "nodemon": "^2.0.15",
    "nuxt3": "^3.0.0-27409835.c53c736",
    "rimraf": "^3.0.2",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0"
  }
}
